// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "prisma://foo?api_key=123"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}